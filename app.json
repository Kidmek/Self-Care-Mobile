{
  "expo": {
    "name": "እኔ ለእኔ",
    "slug": "SelfCare",
    "version": "1.0.0",
    "scheme": "selfcare",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "14.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.jpg",
      "backgroundColor": "#add1d2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.jpg",
        "backgroundColor": "#add1d2"
      },
      "package": "com.bandaw.SelfCare"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "499ebcd7-747e-4f80-99a7-3cf876759146"
      }
    }
  }
}
